<h1>Listing project_organizations</h1>

<table>
  <thead>
    <tr>
      <th>Project</th>
      <th>Organization</th>
      <th>Project organization </th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @project_organizations.each do |project_organization| %>
      <tr>
        <td><%= project_organization.project %></td>
        <td><%= project_organization.organization %></td>
        <td><%= project_organization.project_organization_ %></td>
        <td><%= link_to 'Show', project_organization %></td>
        <td><%= link_to 'Edit', edit_project_organization_path(project_organization) %></td>
        <td><%= link_to 'Destroy', project_organization, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Project organization', new_project_organization_path %>
