<h1>Listing project_team_employee_reports</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Start date</th>
      <th>End date</th>
      <th>Report status</th>
      <th>Employee</th>
      <th>Project</th>
      <th>Project team</th>
      <th>Project team role</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @project_team_employee_reports.each do |project_team_employee_report| %>
      <tr>
        <td><%= project_team_employee_report.name %></td>
        <td><%= project_team_employee_report.start_date %></td>
        <td><%= project_team_employee_report.end_date %></td>
        <td><%= project_team_employee_report.report_status %></td>
        <td><%= project_team_employee_report.employee %></td>
        <td><%= project_team_employee_report.project %></td>
        <td><%= project_team_employee_report.project_team %></td>
        <td><%= project_team_employee_report.project_team_role %></td>
        <td><%= link_to 'Show', project_team_employee_report %></td>
        <td><%= link_to 'Edit', edit_project_team_employee_report_path(project_team_employee_report) %></td>
        <td><%= link_to 'Destroy', project_team_employee_report, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

