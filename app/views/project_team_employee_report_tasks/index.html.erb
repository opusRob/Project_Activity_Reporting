<h1>Listing project_team_employee_report_tasks</h1>

<table>
  <thead>
    <tr>
      <th>Project team employee report</th>
      <th>Work date</th>
      <th>Work hours</th>
      <th>Task</th>
      <th>Application</th>
      <th>Release</th>
      <th>Build</th>
      <th>Created by</th>
      <th>Updated by</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @project_team_employee_report_tasks.each do |project_team_employee_report_task| %>
      <tr>
        <td><%= project_team_employee_report_task.project_team_employee_report %></td>
        <td><%= project_team_employee_report_task.work_date %></td>
        <td><%= project_team_employee_report_task.work_hours %></td>
        <td><%= project_team_employee_report_task.task %></td>
        <td><%= project_team_employee_report_task.application %></td>
        <td><%= project_team_employee_report_task.release %></td>
        <td><%= project_team_employee_report_task.build %></td>
        <td><%= project_team_employee_report_task.created_by %></td>
        <td><%= project_team_employee_report_task.updated_by %></td>
        <td><%= link_to 'Show', project_team_employee_report_task %></td>
        <td><%= link_to 'Edit', edit_project_team_employee_report_task_path(project_team_employee_report_task) %></td>
        <td><%= link_to 'Destroy', project_team_employee_report_task, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

