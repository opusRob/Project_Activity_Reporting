<h1>Listing project_teams</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Project</th>
      <th>Is active</th>
      <th>Is deleted</th>
      <th>Deleted at</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @project_teams.each do |project_team| %>
      <tr>
        <td><%= project_team.name %></td>
        <td><%= project_team.project %></td>
        <td><%= project_team.is_active %></td>
        <td><%= project_team.is_deleted %></td>
        <td><%= project_team.deleted_at %></td>
        <td><%= link_to 'Show', project_team %></td>
        <td><%= link_to 'Edit', edit_project_team_path(project_team) %></td>
        <td><%= link_to 'Destroy', project_team, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Project team', new_project_team_path %>
