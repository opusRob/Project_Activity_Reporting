<h1>Listing project_team_employees</h1>

<table>
  <thead>
    <tr>
      <th>Employee</th>
      <th>Project</th>
      <th>Project team role</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @project_team_employees.each do |project_team_employee| %>
      <tr>
        <td><%= project_team_employee.employee %></td>
        <td><%= project_team_employee.project %></td>
        <td><%= project_team_employee.project_team_role %></td>
        <td><%= link_to 'Show', project_team_employee %></td>
        <td><%= link_to 'Edit', edit_project_team_employee_path(project_team_employee) %></td>
        <td><%= link_to 'Destroy', project_team_employee, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Project team employee', new_project_team_employee_path %>
